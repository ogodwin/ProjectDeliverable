        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CKK.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="CKKTitle" Content="Corey's Knick Knacks" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Rectangle HorizontalAlignment="Left" Height="213" Margin="40,78,0,0" Stroke="Black" VerticalAlignment="Top" Width="284"/>
        <TextBox HorizontalAlignment="Left" Margin="54,92,0,0" Text="Username" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" Height="24"/>
        <TextBox HorizontalAlignment="Left" Margin="54,121,0,0" Text="Password" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" Height="24"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="93,180,0,0" VerticalAlignment="Top" Width="178" Height="65"/>
        <Label Content="Don't have an account?" HorizontalAlignment="Left" Margin="114,250,0,0" VerticalAlignment="Top" Width="136" Cursor="Hand"/>

    </Grid>
    <Window.Resources>

    </Window.Resources>
